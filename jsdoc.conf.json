{
      "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
      },
      "source": {
        "includePattern": ".+\\.js(x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
      },
      "plugins": ["node_modules/jsdoc-jsx/index"],
      "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
      }
}
